  "scripts": {
    "build": "rimraf dist && tsc && rollup -c rollup.config.js",
    "build:rollup": "rollup --config --environment NODE_ENV:production",
    "dev": "etsc --bundle --target=chrome",
    "lint": "yarn lint:ts && yarn type-check",
    "lint:fix": "pretty-quick --staged && eslint --cache --fix \"src/**/*.{ts,tsx,json,js}\"",
    "lint:ts": "eslint \"src/**/*.{ts,tsx,json,js}\"",
    "pretty": "prettier --write \"src/**/*.{js,ts,tsx,css,json}\"",
    "type-check": "tsc --noEmit -p tsconfig.json",
    "tsc:watch": "tsc --watch",
    "start:single": "concurrently --kill-others --names tsc,web-dev-server \"npm run tsc:watch\" \"web-dev-server --app-index index.html --node-resolve --open --watch\"",
    "start": "concurrently --kill-others --names tsc,web-dev-server \"npm run tsc:watch\" \"web-dev-server --config web-dev-server.config.js\""
  },
  "main": "dist/index.js",
  "packageManager": "yarn@3.2.0"
